# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<src>                    <mnt_point>  <type>   <mnt_flags and options>                            <fs_mgr_flags>
/dev/block/mmcblk0p17     /system      ext4     ro,barrier=1                                       wait
/dev/block/mmcblk0p15     /cache       ext4     nosuid,nodev,barrier=1                             wait,check
/dev/block/mmcblk0p18     /data        ext4     nosuid,nodev,barrier=1                             wait,check

/devices/platform/msm_sdcc.1/mmc_host /storage/sdcard1 vfat defaults voldmanaged=sdcard:auto

/devices/platform/msm_sdcc.3/mmc_host/mmc0 /storage/sdcard0 vfat defaults voldmanaged=emmc:19,nonremovable
